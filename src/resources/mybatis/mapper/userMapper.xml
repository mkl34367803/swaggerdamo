<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yuanqiao.dao.UserDao">
    <select id="getUserById" parameterType="integer" resultType="com.yuanqiao.entity.User">
        select * from user where id = #{id}
    </select>

